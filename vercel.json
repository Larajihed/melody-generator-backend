{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "functions": {
    "api/resetGenerations.js": {
      "runtime": "nodejs16.x"
    }
  },
  "integration": {
    "middleware": "./vercel.integration.js",
    "scheduling": [
      {
        "name": "resetGenerations",
        "cron": "0 0 * * *"
      }
    ]
  }
}
